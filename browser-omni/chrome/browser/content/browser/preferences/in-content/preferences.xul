                      value="panePrivacy"
                      helpTopic="prefs-privacy"
                      tooltiptext="&panePrivacySecurity.title;"
                      align="center">
          <image class="category-icon"/>
          <label class="category-name" flex="1">&panePrivacySecurity.title;</label>
